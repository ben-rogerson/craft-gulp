{
  "name": "craft-gulp",
  "description": "Craft CMS + SCSS + JS + Gulp",
  "version": "1.0.0",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "start": "gulp",
    "dev": "gulp",
    "clean": "gulp clean"
  },
  "paths": {
    "urls": {
      "dev": "https://craft-gulp.test"
    },
    "templates": "templates/",
    "src": {
      "img": "src/img/",
      "js": "src/js/",
      "scss": "src/scss/",
      "icons": "src/icons/"
    },
    "built": {
      "base": "./",
      "assets": "./public/assets/",
      "js": "./public/assets/js/",
      "css": "./public/assets/css/",
      "img": "./public/assets/img/"
    }
  },
  "vars": {
    "siteCssName": "styles.css",
    "pluginsJsName": "plugins.js"
  },
  "globs": {
    "babelJs": [
      "./src/js/*.js"
    ],
    "globalJs": [
      "node_modules/jquery/dist/jquery.slim.min.js"
    ],
    "inlineJs": [],
    "critical": [
      {
        "url": "",
        "template": "index"
      },
      {
        "url": "blog/example",
        "template": "blog"
      }
    ]
  },
  "dependencies": {
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0-rc.3",
    "babelify": "^9.0.0",
    "browser-sync": "^2.24.6",
    "browserify": "^16.2.2",
    "browserify-shim": "^3.8.14",
    "chalk": "^2.4.1",
    "critical": "1.3.4",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-import": "^2.14.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-bro": "^1.0.3",
    "gulp-cached": "^1.1.1",
    "gulp-changed": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-color": "0.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-header": "^2.0.5",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.1",
    "gulp-size": "^3.0.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^6.1.1",
    "gulp-twig": "^1.2.0",
    "gulp-uglify": "^3.0.1",
    "moment": "^2.22.2",
    "node-notifier": "^5.2.1",
    "os": "^0.1.1"
  }
}
